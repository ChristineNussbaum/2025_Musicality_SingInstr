load("input/VOCAS_all_preprocessed.RData")
# load required packages for the factor analysis
library(psych) #principal function
library(GPArotation) # für Rotationen
library(lm.beta) ## für standardisierte Regressionskoeffizienten
## extract the items and convert them back to wide format:
D_wide <- pivot_wider(D[D$Group != "TR", c(1,2,6)], names_from = "Item", values_from = "Response")
## calculate correlation matrix
d_cor <- cor(D_wide[,-1]) #without the first column
# first PCA, orthogonal rotation
PCA1 <- princomp(covmat = d_cor)
summary(PCA1, loadings = TRUE)
eigen(d_cor)$values # Eigenwerte
# Screeplots
plot(PCA1)
plot(PCA1, type = "lines")
# Anzahl ausgewählter Hauptkomponenten über nfactors angeben,
# Rotationsmethode wählen: varimax (Varimax wird gewählt, weil Faktoren nicht miteinander korrelieren sollen/dürfen)
PCA1_varimax <- principal(d_cor, nfactors = 1, rotate = "varimax")
print(PCA1_varimax) #together
Item_fit<- data.frame(PCA1_varimax$communality) # Kommunalities
names(Item_fit) <- c("Communalites")
Item_fit$uniqueness <- 1- Item_fit$Communalites
Item_good <- Item_fit %>% filter(Communalites > .25)
plot(sort(Item_good$Communalites))
PCA1_varimax$loadings
